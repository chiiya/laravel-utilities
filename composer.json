{
  "name": "chiiya/laravel-utilities",
  "description": "Common utilities for laravel projects",
  "keywords": ["Laravel", "utilities", "common"],
  "homepage": "https://github.com/chiiya/laravel-utilities",
  "license": "MIT",
  "authors": [
    {
      "name": "Elisha Witte",
      "email": "elishawitte@gmail.com",
      "homepage": "https://github.com/chiiya",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1",
    "ext-zip": "*",
    "openspout/openspout": "^4.0",
    "guzzlehttp/guzzle": "^7.4",
    "illuminate/contracts": "^9.0|^10.0|^11.0",
    "spatie/laravel-package-tools": "^1.11"
  },
  "require-dev": {
    "chiiya/laravel-code-style": "^2.3",
    "orchestra/testbench": "^7.2|^8.0"
  },
  "autoload": {
    "psr-4": {
      "Chiiya\\Common\\": "src/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "allow-plugins": {
      "phpro/grumphp": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Chiiya\\Common\\LaravelUtilitiesServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
